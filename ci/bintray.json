{
    "package": {
        "name": "coyim-bin",
        "repo": "coyim",
        "subject": "twstrike",
        "desc": "I was pushed completely automatically",
        "website_url": "https://coy.im",
        "issue_tracker_url": "https://github.com/twstrike/coyim/issues",
        "vcs_url": "https://github.com/twstrike/coyim.git",
        "github_repo": "twstrike/coyim",
        "github_use_tag_release_notes": false,
        "github_release_notes_file": "RELEASE.txt",
        "licenses": ["MIT"],
        "labels": ["instant", "messenger", "gtk", "otr"],
        "public_download_numbers": true,
        "public_stats": true
    },

    "version": {
        "name": "<VERSION>",
        "desc": "",
        "gpgSign": false
    },

    "files": [{
      "includePattern": "bin/(.*)_(.*)_(.*)",
      "uploadPattern": "<RELEASE>/$2/$3/$1",
      "matrixParams": { "override": 1 }
    },
    {
      "includePattern": "bin/(.*).dmg",
      "uploadPattern": "<RELEASE>/mac-bundle/$1.dmg",
      "matrixParams": { "override": 1 }
    }],
    "publish": true
}
